{
  "name": "node6-babel-gulp-boilerplate",
  "version": "1.2.0",
  "description": "node6-babel-gulp-boilerplate",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "watch": "gulp clean && gulp",
    "clean": "gulp clean",
    "build": "gulp clean && gulp build",
    "prepublish": "npm run build",
    "test": "npm run build && mocha --compilers js:babel-core/register",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register"
  },
  "author": "Maxime Sahroui <maxime.sahroui@gmail.com> (http://maximeshr.github.io)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-3": "^6.11.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.1",
    "gulp-filter": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.2"
  }
}
