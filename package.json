{
  "name": "node-babel-gulp-boilerplate",
  "version": "1.1.0",
  "description": "NodeJS boilerplate for scripts using Babel and Gulp",
  "main": "src/index.js",
  "scripts": {
    "start": "pm2 start ./build/index.js --name=boilerplate",
    "watch": "pm2 start ./build/index.js --watch --name=boilerplate",
    "dev": "gulp clean && gulp",
    "build": "gulp clean && gulp build",
    "clean": "gulp clean",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Maxime Sahroui <maxime.sahroui@gmail.com> (http://maximeshr.github.io)",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^2.0.0",
    "gulp": "^3.9.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.13.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.1",
    "gulp-filter": "^4.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0"
  }
}
